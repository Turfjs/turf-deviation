{
  "name": "turf-deviation",
  "version": "1.0.2",
  "description": "turf deviation module",
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js test.js",
    "pretest": "npm run lint",
    "test": "tape test.js",
    "doc": "dox -r < index.js | doxme --readme > README.md"
  },
  "eslintConfig": {
    "extends": "mourner",
    "rules": {
      "strict": [
        1
      ],
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Turfjs/turf-deviation.git"
  },
  "keywords": [
    "turf",
    "geojson",
    "standard",
    "deviation",
    "stats"
  ],
  "author": "morganherlocker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Turfjs/turf-deviation/issues"
  },
  "homepage": "https://github.com/Turfjs/turf-deviation",
  "devDependencies": {
    "benchmark": "^1.0.0",
    "dox": "^0.6.1",
    "doxme": "^1.4.3",
    "eslint": "^1.10.3",
    "eslint-config-mourner": "^1.0.1",
    "tape": "^3.5.0",
    "turf-featurecollection": "^1.0.0",
    "turf-point": "^2.0.0",
    "turf-polygon": "^1.0.2"
  },
  "dependencies": {
    "simple-statistics": "^0.9.0",
    "turf-inside": "^1.1.3"
  }
}
